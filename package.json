  "scripts": {
    "start": "cross-env TAILWIND_MODE=watch craco start",
    "start:local": "cross-env TAILWIND_MODE=watch PORT=30443 craco start",
    "build": "craco build",
    "test": "craco test --watchAll",
    "test:ci": "craco test --watchAll=false --silent --updateSnapshot",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:debug": "eslint --debug \"src/**/*.{ts,tsx,js,jsx}\"",
    "prettier": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,yml,md}\"",
    "prettier:format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,yml,md}\"",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "docker:exec": "docker-compose -f docker-compose.yml up -d --build",
    "docker:stop": "docker-compose stop",
    "eject": "react-scripts eject",
    "storybook": "cross-env TAILWIND_MODE=watch start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css,yml,md}": [
      "npm run prettier:format",
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
